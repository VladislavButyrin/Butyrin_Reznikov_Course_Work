<Window x:Class="TravelAgencyView.WindowRegOrAut"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TravelAgencyView"
        mc:Ignorable="d"
        Title="WindowRegOrAut" Height="220" Width="300">
    <StackPanel Orientation="Vertical" VerticalAlignment="Center">
        <Button x:Name="buttonReg" Margin="20" Height="30" Width="120" Click="buttonReg_Click">Регистрация</Button>
        <Button x:Name="buttonAut" Margin="20" Height="30" Width="120" Click="buttonAut_Click">Авторизация</Button>
    </StackPanel>
</Window>
